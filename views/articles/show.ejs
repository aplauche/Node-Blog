    <body>
        
        <div class="container">
            <h1 class="mb-2"><%= article.title %></h1>
            <div class="text-muted mb-4"><%= article.createdAt.toLocaleDateString() %></div>

                <a href="/" class="btn btn-secondary">All Articles</a>
                <a href="/articles/edit/<%= article.id %>" class="btn btn-info">Edit</a>
            <div class="mt-4">
                <!-- use the - instead of = to output raw html -->
                <%- article.sanitizedHtml %>
            </div>
            
            
        </div>
        

        <script src="" async defer></script>
    </body>
</html>